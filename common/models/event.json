{
  "name": "Event",
  "plural": "events",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "ownerId": {
      "type": "number"
    },
    "addressId": {
      "type": "number"
    },
    "fromDateTime": {
      "type": "date"
    },
    "toDateTime": {
      "type": "date"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "ownerId"
    },
    "groups": {
      "type": "hasMany",
      "model": "Group",
      "foreignKey": "eventId",
      "options": {
        "nestRemoting": true
      },
      "through": "EventGroup"
    },
    "categories": {
      "type": "hasMany",
      "model": "Category",
      "foreignKey": "eventId",
      "options": {
        "nestRemoting": true
      },
      "through": "EventCategory"
    },
    "address": {
      "type": "belongsTo",
      "model": "Address",
      "foreignKey": "addressId"
    }
  },
  "acls": [],
  "methods": {}
}
