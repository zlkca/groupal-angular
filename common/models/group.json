{
  "name": "Group",
  "plural": "groups",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "ownerId": {
      "type": "number"
    },
    "created": {
      "type": "date"
    },
    "modified": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "ownerId"
    },
    "pictures": {
      "type": "hasMany",
      "model": "Picture",
      "foreignKey": "entityId",
      "options": {
        "nestRemoting": true
      }
    },
    "qrcodes": {
      "type": "hasMany",
      "model": "QRCode",
      "foreignKey": "entityId",
      "options": {
        "nestRemoting": true
      }
    },
    "categories": {
      "type": "hasMany",
      "model": "Category",
      "foreignKey": "groupId",
      "options": {
        "nestRemoting": true
      },
      "through": "GroupCategory"
    },
    "events": {
      "type": "hasMany",
      "model": "Event",
      "foreignKey": "groupId",
      "options": {
        "nestRemoting": true
      },
      "through": "EventGroup"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "user",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "super",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
